{
  "name": "daily-trader",
  "version": "1.0.0",
  "description": "AWS Bedrock Claude Streaming Test Suite with bearer token authentication",
  "main": "dist/main.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/main.js",
    "dev": "tsx src/main.ts",
    "dev:watch": "nodemon --exec tsx src/main.ts",
    "test": "tsx src/main.ts",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "mcp:inspect": "npx @modelcontextprotocol/inspector@latest"
  },
  "keywords": [
    "ai",
    "streaming",
    "bedrock",
    "claude",
    "aws",
    "typescript",
    "nodejs"
  ],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.13.1",
  "dependencies": {
    "@aws-sdk/client-bedrock-runtime": "3.844.0",
    "@modelcontextprotocol/sdk": "^1.15.1",
    "better-sqlite3": "^12.2.0",
    "dotenv": "17.2.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "24.0.14",
    "nodemon": "3.1.10",
    "tsx": "4.20.3",
    "typescript": "5.8.3"
  }
}